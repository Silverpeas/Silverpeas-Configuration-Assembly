<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>configuration</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <outputDirectory>silvertrace</outputDirectory>
      <directory>src/main/config/silvertrace</directory>
      <includes>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>initialize</outputDirectory>
      <directory>src/main/config/initialize</directory>
      <includes>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>properties</outputDirectory>
      <directory>src/main/config/properties</directory>
      <includes>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>xmlcomponents</outputDirectory>
      <directory>src/main/config/xmlcomponents</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>dbRepository</outputDirectory>
      <directory>src/main/config/dbRepository</directory>
      <includes>
        <include>**/*.xml</include>
        <include>**/*.sql</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>resources</outputDirectory>
      <directory>src/main/config/resources</directory>
      <includes>
        <include>**/*.*</include>
      </includes>
    </fileSet>
     <fileSet>
      <outputDirectory>xmlspacetemplates</outputDirectory>
      <directory>src/main/config/xmlspacetemplates</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>data</outputDirectory>
      <directory>src/main/config/data</directory>
      <includes>
        <include>**/*.xml</include>
        <include>**/*.war</include>
      </includes>
    </fileSet>
    <fileSet>
      <outputDirectory>data</outputDirectory>
      <directory>src/main/config/data</directory>
      <includes>
        <include>**/*.conf</include>
      </includes>
      <filtered>true</filtered>
    </fileSet>
  </fileSets>
</assembly>